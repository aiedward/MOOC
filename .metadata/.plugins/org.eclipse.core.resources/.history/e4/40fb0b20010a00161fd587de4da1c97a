package JaccardSimil;

public class Keyword {
	int tf;
	double idf;
	double tf_idf;
	String name;
	String topic;
	Keyword(int tf, double idf, String name, String topic){
		this.tf = tf;
		this.idf = idf;
		this.name = name;
		this.topic = topic;
		this.tf_idf = tf*idf;
		
	}
	@Override
	public String toString(){
		String s = "Topic:"+topic+" keyword:"+name+" TF:"+tf+" IDF:"+idf;
		return s;
	}
}
