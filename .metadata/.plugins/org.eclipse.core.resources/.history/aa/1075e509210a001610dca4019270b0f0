package JaccardSimil;

public class Keyword {
	int tf;
	String name;
	
	Keyword(String Name){
		this.name = Name;
		this.tf = 0;
	}

	
	public boolean compare(String target){
		
		
//		if(name.equals(target.toLowerCase())){
//			tf++;
//			return true;
//		}
		
// 		String.contains(name)을 사용할 경우 (스타벅스+이모티콘)의 형태는 스타벅스로 처리되지만, (먹스타그램)의 경우 '먹','먹스타그램'키워드의 frequency가 둘다 증가 할 수 있음. 
		if(target.toLowerCase().contains(name)){
			tf++;
			
		}
		
		return false;
		
	}
	
}
